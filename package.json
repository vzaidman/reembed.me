  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "jade": "^1.11.0",
    "nedb": "^1.8.0",
    "normalize-url": "^1.9.1",
    "request": "^2.81.0",
    "urijs": "^1.18.10"
  },
